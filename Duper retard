-- init

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()

local venyx = library.new("Papengkalbo", 5013109572)

-- first page

local page = venyx:addPage("Dupe", 5012544693)

local section1 = page:addSection("Server Crasher")

local section2 = page:addSection("Others")

section1:addToggle("Auto Crash", nil, function(state)

if state then

_G.on = true

while _G.on

do wait()

        local args = {

    [1] = "Baby Noob"

}

game:GetService("ReplicatedStorage").RemoteEvents.Equip:FireServer(unpack(args))

end

          else

_G.on = false

end

end)

section1:addButton("First", function()

local plr = game.Players.LocalPlayer

local char = plr.Character

for i,v in pairs(plr.Backpack:GetChildren()) do

v.Parent = char;

v.Parent = workspace

end

end)

section1:addButton("Second", function()

local player = game.Players.LocalPlayer

local character = player.Character

character:ClearAllChildren()

character:LoadCharacter()

end)

section1:addButton("Timer", function()

local plr = game.Players.LocalPlayer

local gui = Instance.new("ScreenGui")

gui.Parent = plr.PlayerGui

gui.ResetOnSpawn = true

local timer = Instance.new("TextLabel")

timer.Parent = gui

timer.BackgroundTransparency = 0

timer.Size = UDim2.new(0.125, 0, 0.125, 0)

timer.Position = UDim2.new(0, 0, 0.5 ,0)

timer.TextXAlignment = Enum.TextXAlignment.Center

timer.TextScaled = true

timer.Text = "00:00:00"

timer.TextColor3 = Color3.new(27, 42, 53)

timer.Font = Enum.Font.Code

local x = 0

local y = 0

local z = 0

while wait(1) do

	if z >= 60 then		y = y + 1

		z = 0

	end

	if y >= 60 then

		x = x + 1

		y = 0

	end

	local tz = "00"

	local ty = "00"

	local tx = "00"

	if z >= 10 then

		tz = z

	else

		tz = "0" .. z

	end

	if y >= 10 then

		ty = y

	else

		ty = "0" .. y

	end

	if x >= 10 then

		tx = x

	else

		tx = "0" .. x

	end

	timer.Text = tx .. ":" .. ty .. ":" .. tz

	z = z + 1

end

end)

section2:addKeybind("Toggle Keybind", Enum.KeyCode.E, function()

print("Activated Keybind")

venyx:toggle()

end, function()

print("Changed Keybind")

end)

-- second page

local script = venyx:addPage("Script", 5012544693)

local script = script:addSection("Scripts Adding More Soon")

script:addButton("Infiniteyield", function()

loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()

end)

script:addButton("keyboard", function()

loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()

end)

-- third page

local auto = venyx:addPage("Auto", 5012544693)

local auto = auto:addSection("Auto Drop And Sell")

auto:addToggle("Auto Equip Banana", nil, function(state)

if state then

_G.on = true

while _G.on

do wait(1)

        local args = {

    [1] = "The banana"

}

game:GetService("ReplicatedStorage").RemoteEvents.Equip:FireServer(unpack(args))

end

          else

_G.on = false

end

end)

auto:addToggle("Auto Drop Banana", nil, function(state)

if state then

_G.on = true

while _G.on

do wait(1)

        local args = {

    [1] = "The banana"

}

game:GetService("ReplicatedStorage").RemoteEvents.Drop:FireServer(unpack(args))

end

          else

_G.on = false

end

end)

auto:addToggle("Auto Sell Banana", nil, function(state)

if state then

_G.on = true

while _G.on

do wait(1)

        local args = {

    [1] = "The banana"

}

game:GetService("ReplicatedStorage").RemoteEvents.Sell:FireServer(unpack(args))

end

          else

_G.on = false

end

end)

-- fourth page

local useful = venyx:addPage("Useful", 5012544693)

local useful = useful:addSection("Adding More Soon")

-- load

venyx:SelectPage(venyx.pages[1], true)
